{
    "presets": ["react", ["env", {
        "target": {
            "browsers": ["last 1 Chrome versions"]
        },
        "modules": false,
        "loose": true
    }]],
    "plugins": [
        ["transform-object-rest-spread", { "useBuiltIns": true } ],
        "syntax-dynamic-import",
        "transform-class-properties",
		// "tcomb"
        // "lodash"
    ],
    "env": {
        "test": {
            "plugins": ["transform-es2015-modules-commonjs"]
        }
    }
}
